{
  "extends": "standard",
  "rules": {
    "space-before-function-paren": [
      "error", {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
      }
    ],
    "padded-blocks": "off",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "operator-linebreak": "off"
  },
  "globals": {
    "chrome": true
  }
}

